msgid "Duplicate merge"
msgstr "Fusion de doublon"

#: admin/include/trees_cls.php:1164 admin/include/trees_cls.php:1399
#: admin/include/trees_cls.php:1550
msgid "Manual merge"
msgstr "Fusion manuelle"

#: admin/include/trees_cls.php:1176
